level (id SERIAL, name TEXT)

user (id SERIAL, last_name TEXT, first_name TEXT, email TEXT, password TEXT, role TEXT)

quiz (id SERIAL, title TEXT, description TEXT, date_creation TIMESTAMP, #user(id) INT)

tag (id SERIAL, name TEXT, description TEXT)

question (id SERIAL, entitled TEXT, anecdote TEXT, wiki TEXT, #quiz(id) INT, #level(id) INT, #answer(id) INT)

answer (id SERIAL, entitled TEXT, #question(id) INT)

quiz_belongs_theme (#quiz(id) INT, #theme(id) INT)
